<div id="<%= dom_id tender_crane_selection %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Tender:</strong>
    <%= tender_crane_selection.tender_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Crane rate:</strong>
    <%= tender_crane_selection.crane_rate_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Purpose:</strong>
    <%= tender_crane_selection.purpose %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Quantity:</strong>
    <%= tender_crane_selection.quantity %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Duration days:</strong>
    <%= tender_crane_selection.duration_days %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Wet rate per day:</strong>
    <%= tender_crane_selection.wet_rate_per_day %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Total cost:</strong>
    <%= tender_crane_selection.total_cost %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Sort order:</strong>
    <%= tender_crane_selection.sort_order %>
  </div>
</div>
